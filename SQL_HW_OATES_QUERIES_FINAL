
SELECT *
FROM employee

SELECT emp_no, first_name, last_name, gender
FROM employee

SELECT employee.emp_no, employee.last_name, employee.first_name, employee.gender, salaries.salary
FROM employee
INNER JOIN salaries ON 
employee.emp_no=salaries.emp_no

SELECT emp_no, first_name, last_name, hire_date  
FROM employee
WHERE hire_date >= '1985-12-31'
AND hire_date < '1987-01-01';
 

SELECT employee.first_name, employee.last_name, employee.hire_date, dept_manager.to_date
FROM employee
INNER JOIN dept_manager ON
employee.emp_no=dept_manager.emp_no;

SELECT employee.emp_no, employee.last_name, employee.first_name, dept_manager.dept_no, dept_manager.emp_no, dept_manager.from_date, dept_manager.to_date
FROM employee
INNER JOIN dept_manager ON 
employee.emp_no= dept_manager.emp_no;

SELECT employee.emp_no, employee.last_name, employee.first_name, dept_manager.dept_no, dept_manager.emp_no, dept_manager.from_date, dept_manager.to_date, department.dept_name
FROM employee
INNER JOIN dept_manager ON 
employee.emp_no= dept_manager.emp_no
INNER JOIN department on 
department.dept_no= dept_manager.dept_no

SELECT employee.emp_no, employee.last_name, employee.first_name, dept_manager.dept_no, dept_manager.emp_no, department.dept_name
FROM employee
INNER JOIN dept_manager ON 
employee.emp_no= dept_manager.emp_no
INNER JOIN department on 
department.dept_no= dept_manager.dept_no





SELECT first_name, last_name
FROM employee

SELECT first_name, last_name
FROM employee
Where first_name = 'Hercules'
AND last_name LIKE 'B%';

SELECT employee.emp_no, employee.last_name, employee.first_name, dept_manager.dept_no, dept_manager.emp_no, department.dept_name
FROM employee
INNER JOIN dept_manager ON 
employee.emp_no= dept_manager.emp_no
INNER JOIN department on 
department.dept_no= dept_manager.dept_no
WHERE dept_name = 'Sales';



SELECT employee.emp_no, employee.last_name, employee.first_name, dept_manager.dept_no, dept_manager.emp_no, department.dept_name
FROM employee
INNER JOIN dept_manager ON 
employee.emp_no= dept_manager.emp_no
INNER JOIN department on 
department.dept_no= dept_manager.dept_no
WHERE dept_name = 'Sales' OR dept_name = 'Development';

SELECT last_name, first_name
FROM employee
ORDER BY last_name DESC;

SELECT COUNT (DISTINCT last_name)
FROM employee
ORDER BY last_name DESC AND GROUP BY employee.last_name;


SELECT COUNT(last_name), employee.first_name
FROM employee
GROUP BY last_name
ORDER BY COUNT(last_name) DESC;

SELECT COUNT(last_name), first_name
FROM employee
GROUP BY last_name
ORDER BY COUNT(last_name) DESC;